      "/getteam": {
        "get": {
          "tags": [
            "Teams"
          ],
          "description": "Return details of a given team.",
          "parameters": [
            {
              "name": "teamid",
              "in": "query",
              "description": "ID of team to search for",
              "required": true,
              "schema": {
                "type": "number",
                "example": 1
